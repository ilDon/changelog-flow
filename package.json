{
  "name": "changelog-flow",
  "version": "0.10.0",
  "description": "CLI tool to facilitate creating, updating and maintaining CHANGELOG files.",
  "main": "out/index.js",
  "bin": {
    "bohr-cglg": "out/bin.js",
    "cglg": "out/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ilDon/changelog-flow.git"
  },
  "keywords": [
    "change log",
    "changelog",
    "CLI",
    "command line interface",
    "json",
    "md",
    "semantic versioning",
    "versioning",
    "release manager",
    "git flow"
  ],
  "author": "ilDon",
  "license": "MIT",
  "scripts": {
    "watch": "webpack --config webpack.config.ts --progress --watch",
    "local": "set TESTING=true&& node out/index.js",
    "start": "node out/index.js",
    "build": "webpack --config webpack.config.ts",
    "deploy": "npm run build && npm run start -- --action=newRelease && npm publish && npm run start -- --action=closeRelease"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "fs-extra": "^9.0.1",
    "git-interface": "^2.1.0",
    "git-state": "^4.1.0",
    "inquirer": "6.2.1",
    "node-cmd": "^3.0.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/inquirer": "0.0.43",
    "@types/node": "^10.12.12",
    "@types/yargs": "^15.0.12",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-unicorn": "^23.0.0",
    "ts-loader": "^8.0.12",
    "ts-node": "^7.0.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0"
  }
}